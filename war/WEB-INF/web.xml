<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>RequestDumperServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.RequestDumperServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IPDumperServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.IPDumperServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RefDumperServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.RefDumperServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UUIDServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.UUIDServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DNSServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.DNSServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RandomServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.RandomServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TimestampServlet</servlet-name>
		<servlet-class>com.flourishworks.webutils.TimestampServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RequestDumperServlet</servlet-name>
		<url-pattern>/req</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IPDumperServlet</servlet-name>
		<url-pattern>/ip</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RefDumperServlet</servlet-name>
		<url-pattern>/ref</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UUIDServlet</servlet-name>
		<url-pattern>/uuid</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DNSServlet</servlet-name>
		<url-pattern>/dns</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RandomServlet</servlet-name>
		<url-pattern>/rand</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TimestampServlet</servlet-name>
		<url-pattern>/time</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
